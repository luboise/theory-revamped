{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch localhost",
			"type": "firefox",
			"request": "launch",
			"reAttach": true,
			"preLaunchTask": "npm: start",
			"enableCRAWorkaround": true,

			"url": "http://localhost:3000",
			"pathMappings": [
				{
					"url": "http://localhost:3000/src",
					"path": "${workspaceFolder}/"
				}
			]
		}
	]
}
